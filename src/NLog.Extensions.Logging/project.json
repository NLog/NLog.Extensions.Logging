{
    "version": "1.0.0-rc2-final-2016-05-18",
    "description": "NLog provider for Microsoft.Extensions.Logging",
    "authors": [ "Microsoft", "Julian Verdurmen" ],
    "packOptions": {
        "tags": [ "NLog", "Microsoft.Extensions.Logging", "Microsoft.Framework.Logging", "log", "logfiles", "aspnet5", "dnx" ],
        "projectUrl": "https://github.com/NLog/NLog.Extensions.Logging",
        "licenseUrl": "https://github.com/NLog/NLog.Extensions.Logging/blob/master/LICENSE",
        "iconUrl": "http://nlog-project.org/NConfig.png",
        "repository": {
            "type": "git",
            "url": "git://github.com/NLog/NLog.Extensions.Logging"
        }
    },

    "buildOptions": {
        "warningsAsErrors": true,
        "keyFile": "NLog.snk"
    },
    "dependencies": {
        "Microsoft.Extensions.Logging.Abstractions": "1.0.0-rc2-final",
        "Microsoft.AspNetCore.Hosting": "1.0.0-rc2-final"
    },
    "frameworks": {
        "net451": {
            "frameworkAssemblies": {
                "System.Xml": "4.0.0.0",
                "System.Xml.XmlSerializer": "4.0.0.0"
            },
            "dependencies": {
                "NLog": "4.3.4"
            }
        },
        "netstandard1.0": {
            "dependencies": {
                "NLog":  "4.4.0-beta5"
            },
            "imports": [
                "portable-net45+win8",
                "dnxcore50",
                "dotnet5.4"
            ]
        }
    }
}
